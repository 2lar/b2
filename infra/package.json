{
  "name": "brain2-infra",
  "version": "1.0.0",
  "description": "Infrastructure for Brain2 application",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "clean": "rimraf cdk.out && find lib bin test lambda/authorizer -name '*.js' -not -path '*/node_modules/*' -not -name 'jest.config.js' -delete 2>/dev/null || true && find lib bin test lambda/authorizer -name '*.d.ts' -not -path '*/node_modules/*' -delete 2>/dev/null || true",
    "typecheck": "tsc --noEmit",
    "compile": "tsc",
    "precompile": "npm run clean",
    "build": "npm run typecheck && npm test && npm run synth",
    "build:with-env": "source ../scripts/load-env.sh infra && npm run build",
    "watch": "npm run test:watch",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "synth": "cdk synth --quiet",
    "synth:with-env": "source ../scripts/load-env.sh infra && npm run synth",
    "deploy": "cdk deploy",
    "deploy:with-env": "source ../scripts/load-env.sh infra && npm run deploy",
    "destroy": "cdk destroy",
    "destroy:with-env": "source ../scripts/load-env.sh infra && npm run destroy"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "aws-cdk": "2.118.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.114.1-alpha.0",
    "@supabase/supabase-js": "^2.50.0",
    "aws-cdk-lib": "^2.118.0",
    "constructs": "^10.4.2",
    "dotenv": "^16.5.0",
    "source-map-support": "^0.5.21"
  }
}
