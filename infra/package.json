{
  "name": "brain2-infra",
  "version": "1.0.0",
  "description": "Infrastructure for Brain2 application",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc && npm test && npm run synth",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "synth": "cdk synth --quiet",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "aws-cdk": "2.118.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.114.1-alpha.0",
    "@supabase/supabase-js": "^2.50.0",
    "aws-cdk-lib": "^2.118.0",
    "constructs": "^10.4.2",
    "dotenv": "^16.5.0",
    "source-map-support": "^0.5.21"
  }
}
