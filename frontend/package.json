{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:with-env": "source ../scripts/load-env.sh frontend && npm run dev",
    "clean": "rm -rf node_modules dist",
    "typecheck": "tsc --noEmit",
    "generate-api-types": "openapi-typescript ../openapi.yaml --output src/types/generated/generated-types.ts",
    "build": "npm run clean && npm install && npm run generate-api-types && npm run typecheck && vite build",
    "build:with-env": "source ../scripts/load-env.sh frontend && npm run build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.84.1",
    "react-test-renderer": "^19.1.0",
    "@types/react-test-renderer": "^19.0.0",
    "@types/cytoscape": "^3.21.9",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "openapi-typescript": "^6.7.6",
    "prettier": "^3.3.3",
    "typescript": "^5.8.3",
    "vite": "^5.2.0",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.7.0",
    "cytoscape": "^3.32.0",
    "cytoscape-cola": "^2.5.1",
    "framer-motion": "^12.23.12",
    "lodash-es": "^4.17.21",
    "normalizr": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.7"
  }
}
